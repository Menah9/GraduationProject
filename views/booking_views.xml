<odoo>
  <data>
    <record id="view_gp_booking_tree" model="ir.ui.view">
      <field name="name">gp.booking.tree</field>
      <field name="model">gp.booking</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="chalet_id"/>
          <field name="partner_id"/>
          <field name="start_date"/>
          <field name="end_date"/>
          <field name="state"/>
        </tree>
      </field>
    </record>

    <record id="view_gp_booking_form" model="ir.ui.view">
      <field name="name">gp.booking.form</field>
      <field name="model">gp.booking</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>
              <field name="chalet_id"/>
              <field name="partner_id"/>
            </group>
            <group>
              <field name="start_date"/>
              <field name="end_date"/>
              <field name="state"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
  </data>
</odoo>
